{
  "name": "gtex-server",
  "version": "1.0.0",
  "description": "GTEx Portal MCP Server - Comprehensive access to GTEx (Genotype-Tissue Expression) genomics data through 25 specialized tools for expression analysis, eQTL/sQTL analysis, and gene/variant lookups",
  "private": true,
  "type": "module",
  "bin": {
    "gtex-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "dev": "npm run build && node build/index.js",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "test": "npm run build && echo 'Testing GTEx server compilation and basic functionality...'"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "gtex",
    "genomics",
    "gene-expression",
    "eqtl",
    "sqtl",
    "bioinformatics",
    "genetics"
  ],
  "author": "Anonymous",
  "license": "MIT"
}
